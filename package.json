{
  "name": "react-app-typescript",
  "version": "1.0.0",
  "description": "react & typescript 結合的開發範本",
  "author": "lightyen",
  "license": "GPL-2.0-only",
  "keywords": [
    "react",
    "typescript",
    "mobx",
    "webpack"
  ],
  "scripts": {
    "tsc-webpack-config": "tsc --project .webpack",
    "webpack-development": "yarn run tsc-webpack-config && webpack --config .webpack/webpack.dev",
    "webpack-production": "yarn run tsc-webpack-config && webpack --config .webpack/webpack.prod",
    "webpack-dll": "yarn run tsc-webpack-config && webpack --config .webpack/webpack.dll",
    "webpack-analyzer": "yarn run tsc-webpack-config && webpack --config .webpack/webpack.analyzer",
    "hot": "yarn run tsc-webpack-config && webpack-dev-server --config .webpack/webpack.hot",
    "start": "yarn run hot",
    "test": "yarn run webpack-development",
    "build": "yarn run webpack-production"
  },
